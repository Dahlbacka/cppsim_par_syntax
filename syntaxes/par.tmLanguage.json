{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "cppsim par",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"comments": {
			"patterns": [{
				"name": "comment.line.double-slash",
				"match": "//.*"
			}]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.other",
				"match": "(num_sim_steps:|Ts:|output:|probe:|probe64:|global_nodes:|global_param:|top_param:|alter:|inp_timing:|inp_dig:|mex_prototype:|simulink_prototype:|library_map_for_code:|add_top_verilog_library_file_statements:|add_bottom_verilog_library_file_statements:|add_verilog_test_file_statements:|add_verilog_test_module_statements:|allow_non_unit_time_for_gtkwave:|allow_verilog_output_clashing:|allow_non_bool_signals_in_bus:|electrical_integration_damping_factor:|temperature_celsius_for_noise_calc:)"
			}]
		}
	},
	"scopeName": "source.par"
}